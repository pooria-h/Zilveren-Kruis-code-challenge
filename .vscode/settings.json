{
    "editor.tabSize": 2,
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/node_modules": true
    },​​
    "eslint.validate": [
        "typescript",
        "typescriptreact",
        "javascript",
        "javascriptreact",
        "vue"
    ],
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "vetur.format.enable": false,
    "css.validate": false,
    "less.validate": false,
    "scss.validate": false
}
